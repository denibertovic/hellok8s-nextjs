{
  "name": "hellok8s-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:scripts": "tsup scripts/createsuperuser.ts --format esm --target node20 --minify --out-dir build-scripts",
    "check": "next lint && tsc --noEmit",
    "clear-rate-limits": "tsx scripts/clear-rate-limits.ts",
    "createsuperuser": "tsx scripts/createsuperuser.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:setup": "tsx scripts/setup-test-db.ts",
    "test:teardown": "tsx scripts/teardown-test-db.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "bcryptjs": "^2.4.3",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.41.0",
    "express-rate-limit": "^8.0.0",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "postgres": "^3.4.4",
    "rate-limit-redis": "^4.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^5.6.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.51",
    "@eslint/eslintrc": "^3.3.1",
    "@google/gemini-cli": "^0.1.12",
    "@openai/codex": "^0.6.0",
    "@sourcegraph/amp": "^0.0.1752609840-gbf1029",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/ui": "^3.2.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.2.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "yarn@4.9.1"
}
